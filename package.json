{
  "name": "test-rebase",
  "version": "0.0.0",
  "description": "Test semantic -release",
  "repository": {
    "type": "git",
    "url": "https://github.com/gurgenyegoryan/test-rebase.git"
  },
  "author": "Gurgen Yegoryan",
  "license": "Apache2.0",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/error": "^4.0.0",
    "@semantic-release/exec": "github:semantic-release/exec",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "conventional-changelog-eslint": "^3.0.9",
    "semantic-release": "^20.1.3"
  },
  "release": {
    "branches": [
      "main"
    ],
    "debug": true,
    "ci": true,
    "dryRun": false,
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint",
          "releaseRules": [
            {
              "tag": "Add",
              "release": "minor"
            },
            {
              "tag": "Improve",
              "release": "patch"
            },
            {
              "tag": "Make",
              "release": "patch"
            },
            {
              "tag": "Refactor",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "eslint",
          "releaseRules": [
            {
              "tag": "Add",
              "release": "minor"
            },
            {
              "tag": "Improve",
              "release": "patch"
            },
            {
              "tag": "Make",
              "release": "patch"
            },
            {
              "tag": "Refactor",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false,
          "assets": [
            {
              "path": "/home/runner/work/test-rebase/test-rebase/VERSION",
              "name": "cat-tracker-nRF9160DK-nbiot-nodebug-${nextRelease.gitTag}.hex",
              "label": "Pre-build HEX file for nRF9160 DK NB-IoT"
            }
          ]
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "VERSION",
            "conanfile.py",
            "package.json",
            "Cargo.toml"
          ],
          "message": "Build: Released ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}